view {
  padding: 0;
  margin: 0;
}
.detailed {
  height: 100%;
  width: 100%;
  overflow: hidden;
}
.detailed .header {
  height: 300rpx;
  width: 100%;
  background: linear-gradient(180deg, #ffcd00, #fff 90%);
}
.detailed .header .total {
  width: 100%;
  height: 120rpx;
  display: flex;
  flex-direction: row;
  padding: 20rpx;
}
.detailed .header .total .selected-date .year {
  font-size: 12px;
}
.detailed .header .total .selected-date .month {
  font-size: 14px;
  border-right: 1px solid #6d6d6d;
  padding-right: 15px;
}
.detailed .header .total .selected-date .month .month-con {
  font-size: 20px;
}
.detailed .header .total .selected-date .month .arrow {
  display: inline-block;
  border-bottom: 10rpx transparent solid;
  border-right: 10rpx transparent solid;
  border-top: 10rpx #333 solid;
  border-left: 10rpx transparent solid;
}
.detailed .header .total .total-con {
  flex: 1;
}
.detailed .header .total .total-con ul {
  height: 150rpx;
  width: 80%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.detailed .header .total .total-con ul li {
  display: flex;
  flex-direction: column;
  margin-left: 50rpx;
}
.detailed .header .total .total-con ul li span {
  font-size: 12px;
}
.detailed .header .total .total-con ul li .num {
  font-size: 16px;
  margin-top: 20rpx;
}
.detailed .header .menu {
  background: #fff;
  width: 700rpx;
  height: 100rpx;
  margin: auto;
  border-radius: 15rpx;
  box-shadow: 5rpx 5rpx #c5c5c5;
}
.detailed .header .menu ul {
  display: flex;
  flex-direction: row;
  padding: 10rpx 20rpx 0rpx;
}
.detailed .header .menu ul li {
  display: flex;
  flex-direction: column;
  margin-right: 50rpx;
  text-align: center;
}
.detailed .header .menu ul li:last-child {
  margin-right: 0rpx;
}
.detailed .header .menu ul li span {
  display: block;
  font-size: 12px;
}
.detailed .header .menu ul li .iconfont {
  color: #ffcd00;
  text-align: center;
  font-size: 20px;
}
.detailed .section {
  width: 100%;
  height: 770rpx;
  overflow: auto;
}
.detailed .section ul {
  display: flex;
  flex-direction: column;
  margin-bottom: 50rpx;
}
.detailed .section ul .date-desc {
  font-size: 12px;
  color: #929292;
  height: 50rpx;
  border-bottom: 1rpx solid #dbdbdb;
  position: relative;
}
.detailed .section ul .date-desc span {
  margin-left: 20rpx;
}
.detailed .section ul .date-desc span:last-child {
  position: absolute;
  right: 20rpx;
}
.detailed .section ul .section-item {
  display: flex;
  flex-direction: row;
}
.detailed .section ul .section-item > span {
  display: inline-block;
  width: 70rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 70rpx;
  background: #dfe4de;
  color: #b99503;
  text-align: center;
  font-size: 20px;
  margin-top: 30rpx;
  margin-left: 20rpx;
}
.detailed .section ul .section-item .item-con {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-self: space-between;
  border-bottom: 1rpx solid #dbdbdb;
  padding: 30rpx 0;
  position: relative;
  margin-left: 20rpx;
}
.detailed .section ul .section-item .item-con span {
  display: inline-block;
  margin-left: 20rpx;
}
.detailed .section ul .section-item .item-con span:last-child {
  position: absolute;
  right: 30rpx;
}
.detailed .section ul .section-item:last-child .item-con {
  border-bottom: 1rpx solid transparent;
}
.detailed .date-con {
  position: absolute;
  bottom: 0;
  height: 400rpx;
  width: 100%;
}
.detailed .date-con .calendar {
  height: 300rpx;
  width: 100%;
}
.detailed .date-con .btn {
  display: flex;
  flex-direction: row;
  justify-self: flex-end;
}
.detailed .date-con .btn button {
  color: #8a8a8a;
  font-size: 14px;
  font-weight: normal;
}
.detailed .date-con .btn .save-btn {
  color: #fff;
  background-color: #ffcd00;
}
